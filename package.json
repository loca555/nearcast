{
  "name": "nearcast",
  "version": "1.0.0",
  "description": "NearCast — предсказательные рынки на NEAR с AI-оракулом",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "node --watch-path=backend backend/server.js",
    "dev:frontend": "npx vite --port 3001",
    "build": "npx vite build",
    "start": "node backend/server.js",
    "build:contract": "cd contract && npm run build"
  },
  "dependencies": {
    "@near-wallet-selector/core": "^10.1.4",
    "@near-wallet-selector/modal-ui": "^10.1.4",
    "@near-wallet-selector/my-near-wallet": "^10.1.4",
    "@reclaimprotocol/zk-fetch": "^0.8.0",
    "better-sqlite3": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "near-api-js": "^6.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "vite": "^5.4.0",
    "vite-plugin-node-polyfills": "^0.25.0"
  }
}
